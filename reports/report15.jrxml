	<queryString language="SQL">
		<![CDATA[select a.*,c.sifra as sifraProizvoda,c.*,e.*
from materijal a 
inner join clan b 
on b.materijal=a.sifra
inner join proizvod c
on c.sifra=b.proizvod
inner join clan1 d 
on c.sifra=d.proizvod
inner join rad e 
on d.rad=e.sifra
where e.nazivrada = 'rad' ;]]>
	</queryString>
